            "example": "2019-01-01T00:00:00.000Z"
        },
        "delivery_time_block_from": {
            "type": "string",
            "format": "date-time",
            "description": "delivery time block from of the picklist",
            "example": "2019-01-01T00:00:00.000Z"
        },
        "delivery_time_block_to": {
            "type": "string",
            "format": "date-time",
            "description": "delivery time block to of the picklist",
